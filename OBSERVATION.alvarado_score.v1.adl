	>

definition
	OBSERVATION[at0000] matches {	-- Alvarado appendicitis score
		data matches {
			HISTORY[at0001] matches {	-- *Event Series(en)
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- *Any event(en)
						data matches {
							ITEM_TREE[at0003] matches {	-- *Tree(en)
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Ömhet höger fossa?
										value matches {
											0|[local::at0014], 	-- Nej
											2|[local::at0015]  	-- Ja
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Förhöjd temperatur (>37.3°C eller 99.1°F)?
										value matches {
											0|[local::at0016], 	-- Nej
											1|[local::at0017]  	-- Ja
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- Släppömhet?
										value matches {
											0|[local::at0018], 	-- Nej
											1|[local::at0019]  	-- Ja
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {	-- Smärtvandring till höger fossa?
										value matches {
											0|[local::at0020], 	-- Nej
											1|[local::at0021]  	-- Ja
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {	-- Anorexi eller aptitlöshet?
										value matches {
											0|[local::at0022], 	-- Nej
											1|[local::at0023]  	-- Ja
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- Illamående eller kräkning?
										value matches {
											0|[local::at0024], 	-- Nej
											1|[local::at0025]  	-- Ja
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- LPK > 10,000?
										value matches {
											0|[local::at0026], 	-- Nej
											2|[local::at0027]  	-- Ja
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {	-- Vänsterförskjuten diff?
										value matches {
											0|[local::at0028], 	-- Nej
											1|[local::at0029]  	-- Ja
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {	-- Total poäng
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..10|}
											}
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {	-- Kommentar
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

