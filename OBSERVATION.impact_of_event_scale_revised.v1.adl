													0|[local::at0105], 	-- Not at all
													1|[local::at0106], 	-- A little bit
													2|[local::at0107], 	-- Moderately
													3|[local::at0108], 	-- Quite a bit
													4|[local::at0110]  	-- Extremely
												}
											}
											ELEMENT[at0109] occurrences matches {0..1} matches {	-- I was jumpy and easily startled
