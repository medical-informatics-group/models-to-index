										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0006] occurrences matches {0..1} matches {	-- I avoided letting myself get upset when I thought about it or was reminded of it
												value matches {
													0|[local::at0007], 	-- Not at all 
													1|[local::at0008], 	-- A little bit 
													2|[local::at0009], 	-- Moderately
													3|[local::at0010], 	-- Quite a bit 
													4|[local::at0011]  	-- Extremely
												}
											}
											ELEMENT[at0012] occurrences matches {0..1} matches {	-- I felt as if it hadn't happened or wasn't real
												value matches {
													0|[local::at0013], 	-- Not at all
													1|[local::at0014], 	-- A little bit
													2|[local::at0015], 	-- Moderately
													3|[local::at0016], 	-- Quite a bit
													4|[local::at0018]  	-- Extremely
												}
											}
											ELEMENT[at0017] occurrences matches {0..1} matches {	-- I stayed away from reminders about it
												value matches {
													0|[local::at0019], 	-- Not at all
													1|[local::at0020], 	-- A little bit
													2|[local::at0021], 	-- Moderately
													3|[local::at0022], 	-- Quite a bit 
													4|[local::at0024]  	-- Extremely
												}
											}
											ELEMENT[at0023] occurrences matches {0..1} matches {	-- I tried not to think about it
												value matches {
													0|[local::at0025], 	-- Not at all
													1|[local::at0026], 	-- A little bit
													2|[local::at0027], 	-- Moderately
													3|[local::at0028], 	-- Quite a bit
													4|[local::at0030]  	-- Extremely
												}
											}
											ELEMENT[at0029] occurrences matches {0..1} matches {	-- I was aware that I still had a lot of feelings about it, but I didn't deal with them
												value matches {
													0|[local::at0031], 	-- Not at all
													1|[local::at0032], 	-- A little bit
													2|[local::at0033], 	-- Moderately
													3|[local::at0034], 	-- Quite a bit
													4|[local::at0036]  	-- Extremely
												}
											}
											ELEMENT[at0035] occurrences matches {0..1} matches {	-- My feelings about it were kind of numb
												value matches {
													0|[local::at0037], 	-- Not at all
													1|[local::at0038], 	-- A little bit
													2|[local::at0039], 	-- Moderately
													3|[local::at0040], 	-- Quite a bit
													4|[local::at0042]  	-- Extremely
												}
											}
											ELEMENT[at0041] occurrences matches {0..1} matches {	-- I tried to remove it from my memory
												value matches {
													0|[local::at0043], 	-- Not at all
													1|[local::at0044], 	-- A little bit
													2|[local::at0045], 	-- Moderately
													3|[local::at0046], 	-- Quite a bit
													4|[local::at0048]  	-- Extremely
												}
											}
											ELEMENT[at0047] occurrences matches {0..1} matches {	-- I tried not to talk about it
												value matches {
													0|[local::at0049], 	-- Not at all
													1|[local::at0050], 	-- A little bit
													2|[local::at0051], 	-- Moderately
													3|[local::at0052], 	-- Quite a bit
													4|[local::at0053]  	-- Extremely
												}
											}
										}
									}
									CLUSTER[at0054] occurrences matches {0..1} matches {	-- Intrusion
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0055] occurrences matches {0..1} matches {	-- Any reminder brought back feelings about it
												value matches {
													0|[local::at0056], 	-- Not at all
													1|[local::at0057], 	-- A little bit
													2|[local::at0058], 	-- Moderately
													3|[local::at0059], 	-- Quite a bit
													4|[local::at0061]  	-- Extremely
												}
											}
											ELEMENT[at0060] occurrences matches {0..1} matches {	-- I had trouble staying asleep
												value matches {
													0|[local::at0062], 	-- Not at all
													1|[local::at0063], 	-- A little bit
													2|[local::at0064], 	-- Moderately
													3|[local::at0065], 	-- Quite a bit
													4|[local::at0067]  	-- Extremely
												}
											}
											ELEMENT[at0066] occurrences matches {0..1} matches {	-- Other things kept making me think about it
												value matches {
													0|[local::at0068], 	-- Not at all
													1|[local::at0069], 	-- A little bit
