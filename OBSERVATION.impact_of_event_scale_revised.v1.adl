													0|[local::at0111], 	-- Not at all
													1|[local::at0112], 	-- A little bit
													2|[local::at0113], 	-- Moderately
													3|[local::at0114], 	-- Quite a bit
													4|[local::at0115]  	-- Extremely
												}
											}
											ELEMENT[at0116] occurrences matches {0..1} matches {	-- I had trouble falling asleep
												value matches {
													0|[local::at0117], 	-- Not at all
													1|[local::at0118], 	-- A little bit
													2|[local::at0119], 	-- Moderately
													3|[local::at0120], 	-- Quite a bit
													4|[local::at0121]  	-- Extremely
												}
											}
											ELEMENT[at0122] occurrences matches {0..1} matches {	-- I had trouble concentrating
												value matches {
													0|[local::at0123], 	-- Not at all
													1|[local::at0124], 	-- A little bit
													2|[local::at0125], 	-- Moderately
													3|[local::at0126], 	-- Quite a bit
													4|[local::at0128]  	-- Extremely
												}
											}
											ELEMENT[at0127] occurrences matches {0..1} matches {	-- Reminders of it caused me to have physical reactions, such as sweating, trouble breathing, nausea, or a pounding heart
												value matches {
													0|[local::at0129], 	-- Not at all
													1|[local::at0130], 	-- A little bit
													2|[local::at0131], 	-- Moderately
													3|[local::at0132], 	-- Quite a bit
													4|[local::at0134]  	-- Extremely
												}
											}
											ELEMENT[at0133] occurrences matches {0..1} matches {	-- I felt watchful and on guard
												value matches {
													0|[local::at0135], 	-- Not at all
													1|[local::at0136], 	-- A little bit
													2|[local::at0137], 	-- Moderately
													3|[local::at0138], 	-- Quite a bit
													4|[local::at0139]  	-- Extremely
												}
											}
										}
									}
									CLUSTER[at0141] occurrences matches {0..1} matches {	-- Results
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0142] occurrences matches {0..1} matches {	-- Avoidance mean
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..4|}
													}
												}
											}
											ELEMENT[at0143] occurrences matches {0..1} matches {	-- Intrusion mean
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..4|}
													}
												}
											}
											ELEMENT[at0144] occurrences matches {0..1} matches {	-- Hyperarousal mean
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..4|}
													}
												}
											}
											ELEMENT[at0145] occurrences matches {0..1} matches {	-- Total mean IES-R Score
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..12|}
													}
												}
											}
											ELEMENT[at0146] occurrences matches {0..1} matches {	-- Total IES-R Score
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..88|}
													}
												}
											}
											ELEMENT[at0147] occurrences matches {0..1} matches {	-- IES-R score consequence
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0148, 	-- No PTSD concern
															at0149, 	-- PTSD is a clinical concern
															at0150, 	-- Probable PTSD diagnosis
															at0151]	-- PTSD very probable + immune system suppression
														}
													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

