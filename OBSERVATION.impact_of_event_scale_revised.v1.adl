										}
									}
									CLUSTER[at0102] occurrences matches {0..1} matches {	-- Hyperarousal
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0104] occurrences matches {0..1} matches {	-- I felt irritable and angry
