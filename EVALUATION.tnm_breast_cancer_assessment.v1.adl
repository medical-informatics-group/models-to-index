definition
	EVALUATION[at0000] matches {	-- Tnm breast cancer assessment
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- 5 Year Survival rates
						value matches {
							0|[local::at0003], 	-- 99% Stage 0
							1|[local::at0004], 	-- 92% Stage I
							2|[local::at0005], 	-- 82% Stage IIA
							3|[local::at0006], 	-- 65% Stage IIB
							4|[local::at0007], 	-- 47% Stage IIIA
							5|[local::at0008], 	-- 44% Stage IIIB
							6|[local::at0009], 	-- 14-44% Stage IIIc
							7|[local::at0010]  	-- 14% Stage IV
						}
					}
				}
			}
		}
	}

