										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {    -- Visual acuity
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0020,    -- 20/200 (Equivalent to 6/60, non-US)
