													at0026,    -- 20/25 (Equivalent to 6/7.5, non-US)
													at0027]    -- 20/20 (Equivalent to 6/6, non-US)
												}
											}
										}
									}
									ELEMENT[at0029] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0028] matches {*}    -- Item tree
		}
	}

