								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Tumörstorlek
										value matches {
											0|[local::at0009], 	-- TX - primärtumör kan inte fastställas.
											1|[local::at0010], 	-- T0 - ingen primärtumör påvisad.
											2|[local::at0011], 	-- Tis - Carcinoma in situ 
											3|[local::at0012], 	-- T1 - (inkluderar T1a, T1b, och T1c)
											4|[local::at0013], 	-- T2 - tumören är större än 2 cm men inte större än 5 cm.
											5|[local::at0014], 	-- T3 - tumören är större än 5 cm.
											6|[local::at0015]  	-- T4 - (inkluderar T4a, T4b, T4c, och T4d)
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Lymfkörtelstatus
										value matches {
											0|[local::at0016], 	-- NX - regionalt lymfkörtelengagemang kan ej fastställas.
											1|[local::at0017], 	-- N0 - inga tumörceller har kunnat påvisas i regionala lymfkörtlar.
											2|[local::at0018], 	-- N0(i+) - spridning påvisad, men < 200 celler eller < 0.2 mm
											3|[local::at0019], 	-- N0(mol+) - cancerceller kan ej lokaliseras i regionala lymfkörtlar men spår av cancerceller har påvisats genom bruk av RT-PCR.
											4|[local::at0021], 	-- N1mi - mikrometastaser i 1-3 lymfkörtlar i axillen; som mest 2mm stora områden (men minst 200 celler/0,2mm).
											5|[local::at0022], 	-- N1a - spridning till 1-3 av axillens lymfkörtlar, med minst ett område större än 2mm.
											6|[local::at0023], 	-- N1b - spridning till bröstets lymfkörtlar, som endast kunnat påvisas genom portvaktsbiopsi (sentinel node biopsy).
											7|[local::at0024], 	-- N1c - både N1a och N1b uppfyllt.
											8|[local::at0025], 	-- N2a - spridning till 4-9 av axillens lymfkörtlar, med minst ett område större än 2mm.
											9|[local::at0026], 	-- N2b - spridning till minst en av bröstets lymfkörtlar, med förstoring av denna/dessa som resultat.
											10|[local::at0027], 	-- N3a
											11|[local::at0028], 	-- N3b
											12|[local::at0029]  	-- N3c
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- Fjärrmetastasering
										value matches {
											0|[local::at0032], 	-- MX - förekomst av fjärrmetastaser kan ej faställas.
											1|[local::at0033], 	-- M0 - inga fjärrmetastaser har kunnat påvisas genom bilddiagnostik eller klinisk undersökning.
											2|[local::at0034], 	-- cM0(i+) - mindre mängd tumörceller har påvisats i blod eller benmärg, eller i lymfkörtlar bortom bröst och portvaktskörtlar (ej större än 0,2mm).
											3|[local::at0035]  	-- M1 - spridning till andra organ; vanligen skelett, lungor, hjärna eller lever.
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {	-- Stadium - bröstcancer
										value matches {
											0|[local::at0036], 	-- Stadium 0
											1|[local::at0037], 	-- Stadium IA
											2|[local::at0038], 	-- Stadium IB
											3|[local::at0039], 	-- Stadium IIA
											4|[local::at0040], 	-- Stadium IIB
											5|[local::at0041], 	-- Stadium IIIA
											6|[local::at0042], 	-- Stadium IIIB
											7|[local::at0043], 	-- Stadium IIIC
											8|[local::at0044]  	-- Stadium IV
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {	-- Kommentar
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

