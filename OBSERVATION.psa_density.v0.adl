definition
	OBSERVATION[at0000] matches {	-- PSA density
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- vilken händelse som helst
						data matches {
							ITEM_TREE[at0003] matches {	-- Träd
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- PSA density
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::350]>
												list = <
													["1"] = <
														units = <"ng/mL2">
														magnitude = <|0.0..<1000.0|>
														precision = <|2|>
													>
												>
											>
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

